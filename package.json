{
  "name": "figma-export-js",
  "version": "1.0.15",
  "description": "Figma Export is a library that allows you to export resources from Figma into your project. It automatically generates a .figma-export.json file that contains settings for exporting resources.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "tsc",
    "prepack": "tsc",
    "start": "tsc && node ./dist/index.js",
    "init": "tsc && node ./dist/index.js init"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urlaunched-com/figma-export-js.git"
  },
  "bin": {
    "figma-export": "./dist/index.js"
  },
  "files": [
    "dist/index.js",
    "dist",
    "README.md",
    "LICENSE",
    ".figma-export.json"
  ],
  "keywords": [
    "figma",
    "export",
    "resources",
    "scss",
    "icons",
    "images"
  ],
  "author": "URLaunched LTD",
  "dependencies": {
    "axios": "^1.6.8",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "sharp": "^0.33.3",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.4.1",
    "types": "^0.1.1",
    "typescript": "^5.4.5"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ]
}
